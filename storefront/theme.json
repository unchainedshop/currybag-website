    "order_detail": {
      "localizations": [
        "/auftrag/:id"
      ],
      "slug": "order_slug",
      "destination": "/order/:id"
    },
    "order": {
      "localizations": [
        "/auftrag"
      ],
      "slug": "order_slug",
      "destination": "/order"
    },
    "account": {
