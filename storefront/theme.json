      ],
      "slug": "email_verified_slug",
      "destination": "/email-verified"
    },
    "imprint": {
      "localizations": [
        "/impressum"
      ],
      "slug": "imprint_slug",
      "destination": "/imprint"
    },
    "login": {
      "localizations": [
        "/anmeldung"
      ],
      "slug": "login_slug",
      "destination": "/login"
    },
    "logout": {
      "localizations": [
        "/ausloggen"
      ],
      "slug": "logout_slug",
      "destination": "/logout"
