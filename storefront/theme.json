    "sign_up": {
      "localizations": [
        "/anmelden"
      ],
      "slug": "sign_up_slug",
      "destination": "/sign-up"
    },
    "thank_you": {
      "localizations": [
        "/danke"
      ],
      "slug": "thank_you_slug",
      "destination": "/thank-you"
